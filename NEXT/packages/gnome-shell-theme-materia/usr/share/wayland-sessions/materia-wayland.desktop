[Desktop Entry]
Name=Materia on Wayland
Comment=This session logs you into GNOME
Exec=env GNOME_SHELL_SESSION_MODE=gnome-materia /usr/bin/gnome-session --session=gnome-materia
TryExec=/usr/bin/gnome-session
Type=Application
DesktopNames=GNOME
X-GDM-SessionRegisters=true
X-Ubuntu-Gettext-Domain=gnome-session-3.0
